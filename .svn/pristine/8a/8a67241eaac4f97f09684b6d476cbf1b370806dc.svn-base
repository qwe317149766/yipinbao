<script>
    apiready = function() {
        window.vm = new Vue({
            el: '#join_platform_partners',
            data: {
                is_apply:0,//申请过得类型
                active:1,
                typeArry:['店掌柜','推广商户','地区合作商'],
                isread:false,//是否阅读

                isdizhi:false,//地址弹框
                iscate:false,//商家分类弹框
                isproject:false,//熟知的项目弹框
                iswhy:false,//为何加盟

                dataobj:{//店掌柜信息
                    realname:'',//真实名称
                    title:'',//公司名称
                    shop_number:'',//商户号
                    // cate:'',//分类
                    // shopsCateId:'',//分类id
                    province:'',//省
                    city:'',//市
                    county:'',//区县
                    address:'',//详细地址
                    mobile:'',//联系电话
                    message:'',//留言
                    yyzz_img:'',
                    zmz_img:'',
                    scz_img:'',
                    logo:'',
                    img1:'',
                    img2:'',
                    img3:'',
                    img4:'',
                    img5:'',
                },

                cateArry:[
                    {'id':'1','title':'男装'},
                    {'id':'2','title':'女装'},
                    {'id':'3','title':'护肤'},
                    {'id':'4','title':'家具'},
                    {'id':'5','title':'母婴'}
                    ],
                projectArry:['社交电商','实体电商','实体店铺','微商保险','工业行业','农业类','文职销售'],
                joinArry:['兼职赚钱','寻求合作','职业推广','着重趋势','列表销售','只是看看'],
                dataobj1:{//推广商户
                    title:'',//商家名称
                    province:'',//省
                    city:'',//市
                    county:'',//区县
                    address:'',//详细地址
                    mobile:'',//联系电话
                    lon:'',//经度
                    lat:'',//纬度
                    // cate:'',//分类
                    // shopsCateId:'',//分类id
                    project:'',//项目
                    why:'',//为何加盟
                    message:'',//留言
                    logo:'',
                    img1:'',
                    img2:'',
                    img3:'',
                    img4:'',
                    img5:'',
                },

                dataobj2:{//地区服务商
                    realname:'',//真实姓名
                    title:'',//公司名称
                    province:'',//省
                    city:'',//市
                    county:'',//区县
                    mobile:'',//联系电话
                    project:'',//项目
                    why:'',//为何加盟
                    message:'',//留言
                    yyzz_img:'',
                    zmz_img:'',
                    scz_img:'',
                    type:2,//2县区代理 1市代理
                },

                dpxieyi:'',//协议
            },
            created: function() {
                var that = this;
                that.getxieyi();
                that.areaList = areaList;
                if(api.pageParam.is_apply){
                    that.is_apply = api.pageParam.is_apply;
                    if(that.is_apply==1){
                        that.active = 1;
                        that.typeArry = ['','推广商户','地区合作商'];
                    }else if(that.is_apply==2){
                        that.active = 2;
                        that.typeArry = ['店掌柜','','地区合作商'];
                    }else if(that.is_apply==3){
                        that.active = 2;
                        that.typeArry = ['','','地区合作商'];
                    }
                }
            },


            methods: {
                getistype:function(value){
                    var that = this;
                    if(value===0){
                        if(that.is_apply==2||that.is_apply==3){
                            that.active = 2;
                        }else {
                            that.active = 1;
                        }
                        that.http(shopCheckOnlineInfo, 'post', {}, function (res) {
                            console.log(JSON.stringify(res));
                            if (res.data.code == 0) {
                                that.active = 0;
                            }else if(res.data.code == 3 ){
                                that.active = 0;
                                that.$toast(res.msg)
                            }else {
                                that.$toast(res.msg)
                            }
                        });
                    }else {
                        that.active = value;
                    }
                },

                //协议
                getxieyi:function(){
                    var that = this;
                    that.http(homeTreaty, 'post', {'type':'shopkeeper'}, function (res) {
                        console.log(JSON.stringify(res));
                        if (res.code == 0) {
                            that.dpxieyi = res.data;
                        }else {
                            that.$toast(res.msg)
                        }
                    })
                },


                //选择上传类型
                uptype:function (nub) {
                    var that = this;
                    api.actionSheet({
                        cancelTitle: '取消',
                        buttons: ['从相册选一张', '拍照'],
                        style:{
                            fontNormalColor:'#666',
                            fontPressColor:'#666',
                        }
                    }, function(ret, err) {
                        var index = ret.buttonIndex;
                        if(index==1) {
                            that.gopic('album',nub);
                        }else if(index==2){
                            that.gopic('camera',nub);
                        }
                    });
                },
                //上传图片
                gopic: function (type,nub) {
                    var that = this;
                    api.getPicture({
                        sourceType: type,
                        encodingType: 'jpg',
                        mediaValue: 'pic',
                        destinationType: 'base64',
                        allowEdit: true,
                        quality: 80,
                        targetWidth: 720,
                        targetHeight: 600,
                        saveToPhotoAlbum: false
                    }, function (ret, err) {
                        if (ret) {
                            if(ret.base64Data){
                                if(that.active==0){
                                    that.dataobj[nub] = ret.base64Data;
                                } else if(that.active==1){
                                    that.dataobj1[nub] = ret.base64Data;
                                }else if(that.active==2){
                                    that.dataobj2[nub] = ret.base64Data;
                                }
                            }
                            // console.log('双向绑定')
                            // console.log(ret.base64Data)
                        } else {
                            console.log(err);
                        }
                    });
                },

                //获取区域
                getarea:function(data){
                    var that = this;
                    that.isdizhi = false;
                    if(that.active == 0){
                        that.dataobj.province = data[0].name;
                        that.dataobj.city = data[1].name;
                        that.dataobj.county = data[2].name;
                        // that.dataobj.code = data[2].code;
                    }else if(that.active == 1){
                        that.dataobj1.province = data[0].name;
                        that.dataobj1.city = data[1].name;
                        that.dataobj1.county = data[2].name;
                    }else if(that.active == 2){
                        that.dataobj2.province = data[0].name;
                        that.dataobj2.city = data[1].name;
                        that.dataobj2.county = data[2].name;
                    }
                },

                //店铺分类
                getcate(value,index) {
                    var that = this;
                    that.iscate=false;
                    // console.log(value);
                    // console.log(index)
                    // if(that.active == 0){
                    //     that.dataobj.cate = value.title;
                    //     that.dataobj.shopsCateId = value.id;
                    // }else
                    if(that.active == 1){
                        that.dataobj1.cate = value.title;
                        that.dataobj1.shopsCateId = value.id;
                    }
                },

                //获取熟知的项目
                gettype(value,index) {
                    var that = this;
                    that.isproject=false;
                    // console.log(value)
                    // console.log(index)
                    if(that.active == 1){
                        that.dataobj1.project = value;
                    }else if(that.active == 2){
                        that.dataobj2.project = value;
                    }
                },

                //为什么加盟
                getwhy(value,index) {
                    var that = this;
                    that.iswhy=false;
                    // console.log(value)
                    // console.log(index)
                    if(that.active == 1){
                        that.dataobj1.why = value;
                    }else if(that.active == 2){
                        that.dataobj2.why = value;
                    }
                },

                //立即入住
                oksumit:function () {
                    var that = this;
                    if(!that.isread){
                        that.$toast('请阅读合同并勾选');
                        return false;
                    }
                    that.$toast.loading({
                        mask: true,
                        loadingType:'spinner',
                        message: '加载中...',
                        duration:20000
                    });
                    var dataobjs = {};
                    if(that.active==0){
                        dataobjs = that.dataobj;
                        that.subdata(dataobjs);
                    }else if(that.active==1){
                        dataobjs = that.dataobj1;
                        // console.log(JSON.stringify(dataobjs));
                        var map = api.require('bMap');
                        map.getCoordsFromName({
                            city: dataobjs.city,
                            address: dataobjs.province + dataobjs.city + dataobjs.county + dataobjs.address
                        }, function(ret, err) {
                            console.log(JSON.stringify(ret));
                            // console.log(JSON.stringify(err));
                            if (ret.status) {
                                dataobjs.lon = ret.lon;
                                dataobjs.lat = ret.lat;
                                that.subdata(dataobjs);
                                // console.log(JSON.stringify(ret));
                            }
                        });
                    }else if(that.active==2){
                        dataobjs = that.dataobj2;
                        that.subdata(dataobjs);
                    }
                    // return;
                    // for(var key in dataobjs){
                    //     if(!dataobjs[key]){
                    //         that.$toast('请完善所有信息');
                    //         return false;
                    //     }
                    // }
                },

                //提交数据
                subdata:function (dataobjs) {
                    var that = this;
                    console.log(JSON.stringify(dataobjs));
                    var url;
                    if(that.active==0){
                        url = shopAddOnlineInfo;
                    }else if(that.active==1){
                        url = shopAddO2oInfo;
                    }else if(that.active==2){
                        url = shopAddDailiInfo;
                    }
                    // console.log(JSON.stringify(dataobjs));
                    that.http(url, 'post', dataobjs, function (res) {
                        console.log(JSON.stringify(res));
                        that.$toast.clear();
                        if (res.code == 0) {
                            api.execScript({//个人中心
                                name: 'root',
                                frameName: 'frame4',
                                script: 'vm.getUserInfo()'
                            });
                            setTimeout(function(){
                                api.openWin({//特殊对待
                                    name: 'audit_certification',
                                    url: './audit_certification.html',
                                    reload: true,
                                    bgColor:'#fff',
                                    slidBackEnabled: false,
                                    animation: {
                                        type: "push", //动画类型（详见动画类型常量）
                                        subType: "from_right", //动画子类型（详见动画子类型常量）
                                        duration: 300 //动画过渡时间，默认300毫秒
                                    },
                                    pageParam: {'type':1}
                                });
                            },300);
                        }else {
                            that.$toast(res.msg)
                        }
                    });
                }

            },
        })
    };
</script>
